<table class="table table-bordered">
<thead>
<tr>
<th>partial_name</th>
<th>Partial Content that You Get</th>
</tr>
</thead>
<tbody>
< for partial in @getCollection("partials").findAll(relativeDirPath: 'general').toJSON(): >
<tr>
<td><- partial.basename ></td>
<td><- partial.content ></td>
</tr>
< end >
</tbody>
</table>
