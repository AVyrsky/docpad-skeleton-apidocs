<!-- FEEDBACK-->
  <div class="modal fade" id="feedbackModal">
    <div id="feedbackPlaceholder"></div>
  </div><!-- /.modal -->
  <script type="text/javascript">
    $('#feedbackPlaceholder').load("<%- @site.feedbackService %>/form/basic?jiraProjectKey=WOO", function(response, status, xhr){
      if(xhr.status === 200){
        $('.feedback-affix').show();
        $("#js-hook-feedback-card").prepend('<li class="grid-cards__element">' +
        '<p class="grid-cards__title"><a data-target="#feedbackModal" data-toggle="modal" title="Send feedback" class="grid-cards__link">Send feedback</a></p>' +
        "<p class='grid-cards__description'>We'd love to hear your opinion, so please submit your  <a data-target='#feedbackModal' data-toggle='modal' title='Send feedback' class='grid-cards__link'>feedback</a> to get in touch with SAP Hybris." +
        '</li>'
        );
        $("#js-hook-feedback-panel").prepend('<li class="grid-cards__element">' +
        '<p class="grid-cards__title"><a data-target="#feedbackModal" data-toggle="modal" title="Send feedback" class="grid-cards__link">Send feedback</a></p>' +
        "<p class='grid-cards__description'>If you find any information that is unclear or incorrect, please  <a data-target='#feedbackModal' data-toggle='modal' title='Send feedback' class='grid-cards__link'>let us know</a> so that we can improve the Dev Portal content." +
        '</li>'
        );
      }
    });
  </script>
<!-- /FEEDBACK -->
